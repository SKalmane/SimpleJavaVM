public class StackTest {
    public static void main(String[] args) {
	ArrayStack stack = new ArrayStack();
	stack.push(10);
	stack.push(20);
	stack.push(30);
	stack.push(40);
	System.out.println(stack.pop());
	System.out.println(stack.pop());
	System.out.println(stack.pop());
	System.out.println(stack.pop());
    }

}
